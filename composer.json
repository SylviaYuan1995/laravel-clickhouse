{
  "name": "sylvia/laravel-clickhouse",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "clickhouse",
    "eloquent"
  ],
  "require": {
    "php": ">=7.2",
    "laravel/framework": "^6.2",
    "the-tinderbox/clickhouse-builder": "4.0.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.0",
    "fzaninotto/faker": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "Sylvia\\LaravelClickHouse\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sylvia\\LaravelClickHouse\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --stop-on-failure tests/"
  }
}
